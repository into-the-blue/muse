{
  "extends": ["plugin:@nx/react", "../../.eslintrc.json"],
  "ignorePatterns": [
    "!**/*",
    ".expo",
    "node_modules",
    "web-build",
    "cache",
    "test-setup.ts"
  ],
  "parserOptions": {
    "project": ["apps/muse-mobile/tsconfig.*?.json"]
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
